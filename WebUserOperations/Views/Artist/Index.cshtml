@using DAL
@model IEnumerable<Artysta>

@{
    ViewData["Title"] = "Index";
}

<h1>Lista Artystów</h1>
<h2>Kliknij na artystę, aby zobaczyć piosenki z jego albumów</h2>

<ul>
    @foreach (var artist in Model)
    {
        <li>@Html.ActionLink(artist.Pseudonim, "Songs", new { artistId = artist.ArtystaID })</li>
    }
</ul>

